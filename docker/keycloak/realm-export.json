{
  "id": "osci",
  "realm": "osci",
  "displayName": "OSCI Platform",
  "displayNameHtml": "<strong>OSCI</strong> Platform",
  "enabled": true,
  "registrationAllowed": false,
  "registrationEmailAsUsername": true,
  "rememberMe": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 5,
  "sslRequired": "none",
  "defaultSignatureAlgorithm": "RS256",
  "revokeRefreshToken": true,
  "refreshTokenMaxReuse": 0,
  "accessTokenLifespan": 300,
  "accessTokenLifespanForImplicitFlow": 900,
  "ssoSessionIdleTimeout": 900,
  "ssoSessionMaxLifespan": 36000,
  "ssoSessionIdleTimeoutRememberMe": 0,
  "ssoSessionMaxLifespanRememberMe": 0,
  "offlineSessionIdleTimeout": 2592000,
  "offlineSessionMaxLifespanEnabled": false,
  "offlineSessionMaxLifespan": 5184000,
  "clientSessionIdleTimeout": 0,
  "clientSessionMaxLifespan": 0,
  "accessCodeLifespan": 60,
  "accessCodeLifespanUserAction": 300,
  "accessCodeLifespanLogin": 1800,
  "actionTokenGeneratedByAdminLifespan": 43200,
  "actionTokenGeneratedByUserLifespan": 300,
  "oauth2DeviceCodeLifespan": 600,
  "oauth2DevicePollingInterval": 5,
  "internationalizationEnabled": false,
  "supportedLocales": [],
  "defaultLocale": "en",
  "authenticationFlows": [
    {
      "id": "browser-flow-osci",
      "alias": "browser-osci",
      "description": "OSCI browser-based authentication with OTP",
      "providerId": "basic-flow",
      "topLevel": true,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "auth-cookie",
          "authenticatorFlow": false,
          "requirement": "ALTERNATIVE",
          "priority": 10,
          "autheticatorFlow": false,
          "userSetupAllowed": false
        },
        {
          "authenticatorFlow": true,
          "requirement": "ALTERNATIVE",
          "priority": 20,
          "autheticatorFlow": true,
          "flowAlias": "browser-osci-forms",
          "userSetupAllowed": false
        }
      ]
    },
    {
      "id": "browser-forms-osci",
      "alias": "browser-osci-forms",
      "description": "Username/password + conditional OTP",
      "providerId": "basic-flow",
      "topLevel": false,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "auth-username-password-form",
          "authenticatorFlow": false,
          "requirement": "REQUIRED",
          "priority": 10,
          "autheticatorFlow": false,
          "userSetupAllowed": false
        },
        {
          "authenticatorFlow": true,
          "requirement": "CONDITIONAL",
          "priority": 20,
          "autheticatorFlow": true,
          "flowAlias": "browser-osci-otp-conditional",
          "userSetupAllowed": false
        }
      ]
    },
    {
      "id": "browser-otp-conditional-osci",
      "alias": "browser-osci-otp-conditional",
      "description": "OTP only if user has it configured",
      "providerId": "basic-flow",
      "topLevel": false,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "conditional-user-configured",
          "authenticatorFlow": false,
          "requirement": "REQUIRED",
          "priority": 10,
          "autheticatorFlow": false,
          "userSetupAllowed": false
        },
        {
          "authenticator": "auth-otp-form",
          "authenticatorFlow": false,
          "requirement": "REQUIRED",
          "priority": 20,
          "autheticatorFlow": false,
          "userSetupAllowed": false
        }
      ]
    },
    {
      "id": "direct-grant-flow",
      "alias": "direct grant",
      "description": "OpenID Connect Resource Owner Grant",
      "providerId": "basic-flow",
      "topLevel": true,
      "builtIn": true,
      "authenticationExecutions": [
        {
          "authenticator": "direct-grant-validate-username",
          "authenticatorFlow": false,
          "requirement": "REQUIRED",
          "priority": 10,
          "autheticatorFlow": false,
          "userSetupAllowed": false
        },
        {
          "authenticator": "direct-grant-validate-password",
          "authenticatorFlow": false,
          "requirement": "REQUIRED",
          "priority": 20,
          "autheticatorFlow": false,
          "userSetupAllowed": false
        },
        {
          "authenticator": "direct-grant-validate-otp",
          "authenticatorFlow": false,
          "requirement": "DISABLED",
          "priority": 30,
          "autheticatorFlow": false,
          "userSetupAllowed": false
        }
      ]
    }
  ],
  "browserFlow": "browser-osci",
  "directGrantFlow": "direct grant",
  "requiredActions": [
    {
      "alias": "CONFIGURE_TOTP",
      "name": "Configure OTP",
      "providerId": "CONFIGURE_TOTP",
      "enabled": true,
      "defaultAction": false,
      "priority": 10,
      "config": {}
    },
    {
      "alias": "UPDATE_PASSWORD",
      "name": "Update Password",
      "providerId": "UPDATE_PASSWORD",
      "enabled": true,
      "defaultAction": false,
      "priority": 30,
      "config": {}
    },
    {
      "alias": "UPDATE_PROFILE",
      "name": "Update Profile",
      "providerId": "UPDATE_PROFILE",
      "enabled": true,
      "defaultAction": false,
      "priority": 40,
      "config": {}
    },
    {
      "alias": "VERIFY_EMAIL",
      "name": "Verify Email",
      "providerId": "VERIFY_EMAIL",
      "enabled": true,
      "defaultAction": false,
      "priority": 50,
      "config": {}
    },
    {
      "alias": "terms_and_conditions",
      "name": "Terms and Conditions",
      "providerId": "terms_and_conditions",
      "enabled": false,
      "defaultAction": false,
      "priority": 20,
      "config": {}
    }
  ],
  "otpPolicyType": "totp",
  "otpPolicyAlgorithm": "HmacSHA1",
  "otpPolicyInitialCounter": 0,
  "otpPolicyDigits": 6,
  "otpPolicyLookAheadWindow": 1,
  "otpPolicyPeriod": 30,
  "otpPolicyCodeReusable": false,
  "otpSupportedApplications": [
    "totpAppGoogleName",
    "totpAppMicrosoftAuthenticatorName",
    "totpAppFreeOTPName"
  ],
  "passwordPolicy": "length(12) and upperCase(1) and lowerCase(1) and digits(1) and specialChars(1) and notUsername(undefined) and passwordHistory(3)",
  "roles": {
    "realm": [
      {
        "id": "role-security-admin",
        "name": "SecurityAdmin",
        "description": "Full system administrator with unrestricted access to all features and settings",
        "composite": false,
        "clientRole": false,
        "containerId": "osci"
      },
      {
        "id": "role-security-manager",
        "name": "SecurityManager",
        "description": "Manages security objects, checklists, tasks, and incidents across all projects",
        "composite": false,
        "clientRole": false,
        "containerId": "osci"
      },
      {
        "id": "role-project-owner",
        "name": "ProjectOwner",
        "description": "Owns and manages specific security objects and their associated checklists",
        "composite": false,
        "clientRole": false,
        "containerId": "osci"
      },
      {
        "id": "role-auditor",
        "name": "Auditor",
        "description": "Read-only access to all data with export capabilities for audit purposes",
        "composite": false,
        "clientRole": false,
        "containerId": "osci"
      },
      {
        "id": "role-developer",
        "name": "Developer",
        "description": "Responds to assigned checklist items and manages own tasks and evidence",
        "composite": false,
        "clientRole": false,
        "containerId": "osci"
      },
      {
        "id": "role-viewer",
        "name": "Viewer",
        "description": "Read-only access to permitted resources",
        "composite": false,
        "clientRole": false,
        "containerId": "osci"
      }
    ]
  },
  "defaultRole": {
    "id": "default-role-osci",
    "name": "default-roles-osci",
    "description": "${role_default-roles}",
    "composite": true,
    "composites": {
      "realm": [
        "Viewer"
      ],
      "client": {
        "account": [
          "view-profile",
          "manage-account"
        ]
      }
    },
    "clientRole": false,
    "containerId": "osci"
  },
  "clients": [
    {
      "id": "client-osci-web",
      "clientId": "osci-web",
      "name": "OSCI Web Application",
      "description": "Angular frontend for the OSCI Platform",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "protocol": "openid-connect",
      "rootUrl": "http://localhost",
      "baseUrl": "http://localhost",
      "redirectUris": [
        "http://localhost/*",
        "http://localhost:4200/*"
      ],
      "webOrigins": [
        "http://localhost",
        "http://localhost:4200",
        "http://localhost:3000"
      ],
      "adminUrl": "http://localhost",
      "frontchannelLogout": true,
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost/*##http://localhost:4200/*",
        "login_theme": "",
        "display.on.consent.screen": "false",
        "oauth2.device.authorization.grant.enabled": "false",
        "backchannel.logout.session.required": "true",
        "backchannel.logout.revoke.offline.tokens": "false",
        "use.refresh.tokens": "true",
        "client_credentials.use_refresh_token": "false",
        "token.response.type.bearer.lower-case": "false",
        "tls-client-certificate-bound-access-tokens": "false",
        "require.pushed.authorization.requests": "false",
        "acr.loa.map": "{}",
        "access.token.lifespan": "300"
      },
      "fullScopeAllowed": true,
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "profile",
        "roles",
        "email",
        "osci-audience"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ],
      "protocolMappers": [
        {
          "id": "mapper-realm-roles",
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String"
          }
        },
        {
          "id": "mapper-audience",
          "name": "audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "osci-api",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        },
        {
          "id": "mapper-subject",
          "name": "subject",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-sub-mapper",
          "consentRequired": false,
          "config": {
            "access.token.claim": "true",
            "id.token.claim": "true",
            "introspection.token.claim": "true"
          }
        }
      ]
    },
    {
      "id": "client-osci-api",
      "clientId": "osci-api",
      "name": "OSCI API",
      "description": "NestJS backend API for the OSCI Platform",
      "enabled": true,
      "bearerOnly": true,
      "publicClient": false,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect",
      "fullScopeAllowed": false,
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "profile",
        "roles",
        "email"
      ],
      "optionalClientScopes": []
    }
  ],
  "clientScopes": [
    {
      "id": "scope-openid",
      "name": "openid",
      "description": "OpenID Connect scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "false",
        "display.on.consent.screen": "true",
        "consent.screen.text": ""
      }
    },
    {
      "id": "scope-email",
      "name": "email",
      "description": "OpenID Connect built-in scope: email",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true",
        "consent.screen.text": "${emailScopeConsentText}"
      },
      "protocolMappers": [
        {
          "id": "mapper-email",
          "name": "email",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "email",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "email",
            "jsonType.label": "String"
          }
        },
        {
          "id": "mapper-email-verified",
          "name": "email verified",
          "protocol": "openid-connect",
